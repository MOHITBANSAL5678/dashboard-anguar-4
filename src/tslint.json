{
    "extends": "../tslint.json",
    "include": ["src/**/*.ts", "node_modules/angular-bootstrap-md/**/*.ts"],    
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "app",
            "kebab-case"
        ]
    }
}
